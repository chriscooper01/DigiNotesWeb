

<section id="contact">
    <div class="container">
        <div class="row">
            <div class="col-lg-8 col-lg-offset-2 text-center">
                <h2 class="section-heading">@ResourceProject.Contact.PageTitle</h2>
                <hr class="primary">
                <p>@ResourceProject.Contact.PageContent</p>
            </div>
            
            <div class="col-lg-8 col-lg-offset-2  text-center">
                <i class="fa fa-envelope-o fa-3x wow bounceIn" data-wow-delay=".1s"></i>
                <p><a href="mailto:contact@diginotes.co.uk">@ResourceProject.Contact.LinkDisplay</a></p>
            </div>
            
        </div>
      
    </div>
</section>
